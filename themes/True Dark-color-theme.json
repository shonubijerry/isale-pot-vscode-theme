	"tokenColors": [
		{
			"scope": "string.quoted.docstring",
			"settings": {
				"foreground": "#2fab6d" // Change this to your desired color (green in this case)
			}
		},
		{
			"scope": "entity.name.type.class",
			"settings": {
				"foreground": "#84c35dfb", // Class name color (red)
				"fontStyle": "bold" // Optionally make it bold
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#f1e42cdb", // Function/method name color (blue)
				"fontStyle": "italic" // Optionally make it italic
			}
		},
		{
			"scope": "variable.other.property", // Targets property access (e.g., id in id.string())
			"settings": {
				"foreground": "#07c7bb" // Level 1 chaining (e.g., id)
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#547a71"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#3190f0"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
